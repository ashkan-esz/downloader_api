{
  "name": "downloader-api",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "type": "module",
  "scripts": {
    "start": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 --experimental-specifier-resolution=node src/server.js",
    "start_limited_ram_traceGcc": "node --optimize_for_size --max_old_space_size=300 --gc_interval=100 --trace_gc --experimental-specifier-resolution=node src/server.js",
    "start_dev": "NODE_ENV=dev nodemon -r dotenv/config --experimental-specifier-resolution=node src/server.js",
    "test": "NODE_ENV=dev node --optimize_for_size --max_old_space_size=460 --gc_interval=100 --experimental-specifier-resolution=node test.js",
    "test_limited_ram": "NODE_ENV=dev node --optimize_for_size --max_old_space_size=250 --gc_interval=100 --experimental-specifier-resolution=node test.js",
    "test_limited_ram_traceGcc": "NODE_ENV=dev node --optimize_for_size --max_old_space_size=250 --gc_interval=100 --trace_gc --experimental-specifier-resolution=node test.js",
    "clinic_profiler_doctor": "clinic doctor --on-port=’autocannon -c100 -d120 http://localhost:3000/updates/movie/news/false/true/1’ -- node --experimental-specifier-resolution=node src/server.js"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.36.0",
    "@aws-sdk/client-s3": "^3.27.0",
    "@sentry/node": "^5.29.2",
    "@sentry/tracing": "^5.30.0",
    "axios": "^0.21.0",
    "axios-retry": "^3.1.9",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^4.2.0",
    "http-errors": "~1.6.3",
    "jwks-rsa": "^1.11.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "mongodb": "^3.6.3",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.0",
    "p-queue": "^6.6.2",
    "persian-rex": "^2.3.3",
    "words-to-numbers": "^1.5.1",
    "ytdl-core": "^4.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.6",
    "object-sizeof": "^1.6.1"
  }
}
