{
  "name": "downloader-api",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "type": "module",
  "scripts": {
    "pre_start": "NODE_ENV=production node src/preStart.js",
    "start": "NODE_ENV=production node --optimize_for_size --max_old_space_size=460 --gc_interval=100 src/server.js",
    "start_limited_ram_traceGcc": "node --optimize_for_size --max_old_space_size=300 --gc_interval=100 --trace_gc src/server.js",
    "start_dev": "NODE_ENV=dev nodemon -r dotenv/config src/server.js",
    "test": "NODE_ENV=dev node --optimize_for_size --max_old_space_size=460 --gc_interval=100 manualTest.js",
    "test_limited_ram": "NODE_ENV=dev node --optimize_for_size --max_old_space_size=250 --gc_interval=100 manualTest.js",
    "test_limited_ram_traceGcc": "NODE_ENV=dev node --optimize_for_size --max_old_space_size=250 --gc_interval=100 --trace_gc manualTest.js",
    "test_manual": "node src/test/manual/index.js",
    "test_jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test_jest_coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage && open ./coverage/lcov-report/index.html",
    "clinic_profiler_doctor": "clinic doctor --on-port=’autocannon -c100 -d120 http://localhost:3000/updates/movie/news/false/true/1’ -- node src/server.js"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.40.0",
    "@aws-sdk/client-s3": "^3.42.0",
    "@sentry/node": "^6.19.6",
    "@sentry/tracing": "^6.19.6",
    "agenda": "^4.2.1",
    "axios": "^0.24.0",
    "axios-retry": "^3.2.4",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.10",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.1",
    "express-validator": "^6.13.0",
    "form-data": "^4.0.0",
    "helmet": "^4.6.0",
    "i18n-iso-countries": "^7.2.0",
    "ip2location-nodejs": "^9.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "mongodb": "^4.3.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.7.1",
    "p-queue": "^6.6.2",
    "persian-rex": "^2.3.3",
    "request-ip": "^2.1.3",
    "sharp": "^0.30.4",
    "uuid": "^8.3.2",
    "words-to-numbers": "^1.5.1",
    "ytdl-core": "^4.11.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.9",
    "jest": "^28.1.0",
    "nodemon": "^2.0.15"
  }
}
