{
  "name": "downloader-api",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "type": "module",
  "scripts": {
    "pre_start": "NODE_ENV=production node src/preStart.js",
    "start": "NODE_ENV=production node --optimize_for_size --max_old_space_size=460 --gc_interval=100 src/server.js",
    "start_limited_ram_traceGcc": "node --optimize_for_size --max_old_space_size=300 --gc_interval=100 --trace_gc src/server.js",
    "start_dev": "NODE_ENV=dev nodemon -r dotenv/config src/server.js",
    "test": "NODE_ENV=dev node manualTest.js",
    "test_limited_ram": "NODE_ENV=dev node --optimize_for_size --max_old_space_size=250 --gc_interval=100 manualTest.js",
    "test_limited_ram_traceGcc": "NODE_ENV=dev node --optimize_for_size --max_old_space_size=250 --gc_interval=100 --trace_gc manualTest.js",
    "test_manual": "node src/test/manual/index.js",
    "test_jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test_jest_coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage && open ./coverage/lcov-report/index.html",
    "clinic_profiler_doctor": "clinic doctor --on-port=’autocannon -c100 -d120 http://localhost:3000/updates/movie/news/false/true/1’ -- node src/server.js",
    "create_sourcemap": "rollup --config rollup.config.js --file bundle.js",
    "upload_sourceamp": "sentry-cli releases files 1 upload-sourcemaps bundle.js"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.162.0",
    "@aws-sdk/client-s3": "^3.165.0",
    "@aws-sdk/lib-storage": "^3.183.0",
    "@mongodb-js/zstd": "^1.0.0",
    "@sentry/node": "^7.12.1",
    "@sentry/tracing": "^7.12.1",
    "agenda": "^4.3.0",
    "axios": "^1.3.4",
    "axios-cookiejar-support": "^4.0.6",
    "axios-retry": "^3.3.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "check-disk-space": "^3.3.1",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-fingerprint": "^1.2.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.6.0",
    "express-validator": "^6.14.2",
    "form-data": "^4.0.0",
    "helmet": "^6.0.0",
    "i18n-iso-countries": "^7.5.0",
    "inquirer": "^9.1.1",
    "ip2location-nodejs": "^9.3.1",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.10.1",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "mongodb": "^4.9.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cache": "^5.1.2",
    "node-os-utils": "^1.3.7",
    "nodemailer": "^6.7.8",
    "p-queue": "^6.6.2",
    "persian-rex": "^2.3.3",
    "request-ip": "^3.3.0",
    "sharp": "^0.32.0",
    "tough-cookie": "^4.1.2",
    "uuid": "^9.0.0",
    "valid-url": "^1.0.9",
    "words-to-numbers": "^1.5.1",
    "ytdl-core": "^4.11.2",
    "zipson": "^0.2.12"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.19.0",
    "@types/node": "^18.7.16",
    "chalk": "^5.2.0",
    "diff": "^5.1.0",
    "jest": "^29.0.2",
    "nodemon": "^2.0.19"
  }
}
