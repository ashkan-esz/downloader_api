{
  "name": "downloader-api",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node --max_old_space_size=1024 src/server.js",
    "start_limited_ram_traceGcc": "node --max_old_space_size=512 --trace_gc src/server.js",
    "start_dev": "NODE_ENV=dev nodemon -r dotenv/config src/server.js",
    "test": "NODE_ENV=dev node manualTest.js",
    "test_light": "NODE_ENV=dev node --optimize_for_size --max_old_space_size=600 manualTest.js",
    "test_limited_ram": "NODE_ENV=dev node --max_old_space_size=512 manualTest.js",
    "test_limited_ram_traceGcc": "NODE_ENV=dev node --max_old_space_size=512 --trace_gc manualTest.js",
    "cli": "node src/test/cli.js",
    "test_manual": "node src/test/manual/index.js",
    "test_jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test_jest_coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage && open ./coverage/lcov-report/index.html",
    "clinic_profiler_doctor": "clinic doctor --on-port=’autocannon -c100 -d120 http://localhost:3000/updates/movie/news/false/true/1’ -- node src/server.js",
    "create_sourcemap": "rollup --config rollup.config.js --file bundle.js",
    "upload_sourceamp": "sentry-cli releases files 1 upload-sourcemaps bundle.js",
    "deploy": "prisma migrate deploy && NODE_ENV=production node --optimize_for_size --max_old_space_size=900 src/server.js",
    "deploy_light": "prisma migrate deploy && NODE_ENV=production node --optimize_for_size --max_old_space_size=600 src/server.js",
    "dev_deploy": "prisma migrate deploy && NODE_ENV=dev nodemon -r dotenv/config src/server.js"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.374.0",
    "@aws-sdk/client-s3": "^3.590.0",
    "@aws-sdk/lib-storage": "^3.590.0",
    "@mongodb-js/zstd": "^1.2.2",
    "@prisma/client": "^5.20.0",
    "@sentry/node": "^7.109.0",
    "@sentry/tracing": "^7.109.0",
    "agenda": "^5.0.0",
    "amqplib": "^0.10.3",
    "axios": "^1.6.8",
    "axios-cookiejar-support": "^4.0.7",
    "axios-retry": "^4.1.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "check-disk-space": "^3.4.0",
    "cheerio": "^1.0.0-rc.12",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "diff": "^5.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-fingerprint": "^1.2.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.2.0",
    "form-data": "^4.0.0",
    "googleapis": "^134.0.0",
    "helmet": "^7.1.0",
    "i18n-iso-countries": "^7.11.0",
    "inquirer": "^9.2.17",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lru-cache": "^10.2.0",
    "mongodb": "^6.8.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-os-utils": "^1.3.7",
    "p-queue": "^8.0.1",
    "persian-rex": "^2.3.3",
    "redis": "^4.6.13",
    "request-ip": "^3.3.0",
    "sharp": "^0.33.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tough-cookie": "^4.1.3",
    "uuid": "^9.0.1",
    "valid-url": "^1.0.9",
    "words-to-numbers": "^1.5.1",
    "yaml": "^2.4.1",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^2.16.0",
    "@types/node": "^20.12.3",
    "@types/swagger-ui-express": "^4.1.6",
    "chalk": "^5.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.20.0"
  }
}
